    style.
      /*! CSS Used from: http://anafi.parrot.com/assets/builds/bundle.css */
      *{margin:0;padding:0;border-collapse:inherit;}
      *,:after,:before{box-sizing:inherit;}
      :not(body){background-repeat:no-repeat;background-position:50%;background-size:cover;}
      *{box-sizing:border-box;}
      .noselect{-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;}
      .nopointer{pointer-events:none;}
      .fixed-background.background{background:#18589d;position:fixed;z-index:0;top:0;left:0;bottom:0;width:100%;overflow:hidden;}
      .fixed-background.background>img{width:100%;height:auto;position:absolute;bottom:0;left:0;will-change:transform;z-index:1;}
      .fixed-background.background>img:nth-child(5){z-index:3;bottom:-2px;}
      .fixed-background.background>img:nth-child(4){bottom:19vw;}
      .fixed-background.background>img:nth-child(3){bottom:50vw;}
      .fixed-background.background>img:first-child,.fixed-background.background>img:nth-child(2){bottom:58vw;}
      @media (min-width:1441px){
      .fixed-background.background>img:nth-child(4){bottom:calc(120px + 7%);}
      .fixed-background.background>img:nth-child(3){bottom:calc(1210px - 39%);}
      .fixed-background.background>img:nth-child(2){bottom:calc(750px + 3%);}
      .fixed-background.background>img:first-child{bottom:calc(740px + 2%);}
      }
      @media (min-width:1701px){
      .fixed-background.background>img:nth-child(4){bottom:calc(240px - 7vw);}
      .fixed-background.background>img:nth-child(3){bottom:calc(620px + 2.6vw);}
      .fixed-background.background>img:first-child,.fixed-background.background>img:nth-child(2){bottom:calc(620px + 7.6vw);}
      }
      .fixed-background.background .filler{position:absolute;z-index:2;left:0;bottom:-45vh;width:100%;height:45vh;background:#000;}
      /*! CSS Used from: https://www.desiringgod.org/dist/application_new.703a7ef6.css ; media=all */
      @media all{
      a,div,footer,form,h1,h2,h3,header,iframe,label,li,nav,p,span,ul{margin:0;padding:0;border:0;font-size:100%;font:inherit;vertical-align:baseline;box-sizing:border-box;}
      footer,header,nav{display:block;}
      ul{list-style:none;}
      a{text-decoration:none;color:inherit;}
      .sans-light{font-weight:200;}
      .sans-light,.sans-normal{font-family:Balto Web,Helvetica,Arial,sans-serif;}
      .sans-normal{font-weight:400;}
      .sans-bold{font-family:Balto Web,Helvetica,Arial,sans-serif;font-weight:700;}
      .fs-4{font-size:48px;}
      .fs-5{font-size:32px;}
      .fs-9{font-size:19px;}
      .fs-10{font-size:16px;}
      .fs-11{font-size:15px;}
      .fs-12{font-size:14px;}
      .fs-13{font-size:13px;}
      .uppercase{text-transform:uppercase;}
      .track-out{letter-spacing:.1em;}
      .lh-tight{line-height:1;}
      .lh-title{line-height:1.25;}
      .container{width:90%;margin:0 auto;max-width:1400px;}
      .grid{display:flex;flex-direction:row;flex-wrap:wrap;}
      [class*=col-]{box-sizing:border-box;display:flex;flex-direction:column;padding:1rem;flex-basis:1;}
      .fb-40{flex-basis:40%;max-width:40%;}
      .fb-60{flex-basis:60%;max-width:60%;}
      .w-100{width:100%;}
      .fb-100{flex-basis:100%;max-width:100%;}
      .col-33{width:33.3333%;}
      .col-66{width:66.6666%;}
      @media screen and (max-width:1024px){
      .col-45-m{width:45%;}
      .col-55-m{width:55%;}
      }
      @media screen and (max-width:640px){
      .fb-100-s{flex-basis:100%;max-width:100%;}
      }
      .row{box-sizing:border-box;display:-webkit-box;display:flex;flex:0 1 auto;-webkit-box-flex:0;-webkit-box-orient:horizontal;-webkit-box-direction:normal;flex-direction:row;flex-wrap:wrap;}
      .col{flex-grow:1;position:relative;}
      @media screen and (max-width:640px){
      [class*=col-]{width:100%;}
      }
      .page-background{
        position:relative;
        z-index:20;
        padding-top:30px;
        background:rgba(245,245,245,0);
        transition:transform .7s cubic-bezier(.19,1,.22,1);
        transform:translateX(0);
      }
      .mb-1{margin-bottom:.5rem;}
      .mt-2{margin-top:1rem;}
      .mb-2{margin-bottom:1rem;}
      .p-3{padding:2rem;}
      .mt-3{margin-top:2rem;}
      .mb-3{margin-bottom:2rem;}
      .pv-4{padding-top:3rem;padding-bottom:3rem;}
      .pr-5{padding-right:4rem;}
      .pl-5{padding-left:4rem;}
      .pv-6{padding-top:5rem;padding-bottom:5rem;}
      @media screen and (max-width:640px){
      .pb-0-s{padding-bottom:0;}
      .pb-2-s{padding-bottom:1rem;}
      .pt-3-s{padding-top:2rem;}
      .pv-4-s{padding-top:3rem;padding-bottom:3rem;}
      .pt-5-s{padding-top:4rem;}
      }
      .block{display:block;}
      .relative{position:relative;}
      @media screen and (max-width:640px){
      .center-s{text-align:center;}
      }
      .bt{border-top:1px solid #dcdcdc;}
      .bl{border-left:1px solid #dcdcdc;}
      @media screen and (max-width:640px){
      .bt-s{border-top:1px solid #dcdcdc;}
      }
      .white{color:#fff;}
      .bg-gray-dark{background-color:#333;}
      .bd-gray{border-color:#555;}
      .gpu{backface-visibility:hidden;perspective:1000;}
      .responsive-video{position:relative;overflow:hidden;padding-bottom:56.25%;height:0;}
      .responsive-video iframe{position:absolute;top:0;left:0;width:100%;height:100%;}
      .share-popup{position:absolute;visibility:hidden;display:none;margin-top:-63px;z-index:30;opacity:0;transition:none;user-select:none;}
      .share-popup .share__options{position:static;display:block;margin:0;width:auto;opacity:1;transition:none;background-color:#333;height:50px;border:0;padding:14px 5px 10px;}
      .share-popup .share__options:after{border-width:6px;margin-left:-6px;margin-top:-2px;top:100%;left:50%;border-top-color:#333;}
      .share-popup .share__option{margin:0 3px;}
      .share-popup .share__option svg{width:20px;height:20px;}
      .share-popup .share__option svg path{fill:#ccc;}
      .share-popup .share__option svg:hover path{fill:#fff;}
      .svg{display:inline-block;vertical-align:top;}
      .svg-facebook{vertical-align:text-top;height:16px;width:16px;}
      .svg-twitter{vertical-align:text-top;margin-left:1px;height:15px;width:15px;}
      @media print{
      .page-background{margin-top:0!important;margin-bottom:0!important;padding-top:0!important;padding-bottom:0!important;}
      .page-background{padding-top:30px!important;}
      }
      }
      /*! CSS Used from: https://www.desiringgod.org/dist/donate.983f599a.css ; media=all */
      @media all{
      .Donate .container{max-width:1080px;}
      .Donate .page-background{background-color:#fff;padding-top:63px;}
      .Donate .js-reach-number{font-family:Menlo,Consolas,Monaco,monospace;font-size:36px;margin-top:1.25rem;}
      .Donate .js-reach-number:after{
        content:"People for one month";
        font-family:Balto Web,Helvetica,Arial,sans-serif;
        font-size:12px;
        font-weight:400;
        display:inline-block;
        width:60px;
        margin-left:10px;
        line-height:1.3;
      }
      .Donate .-submit-btn{box-sizing:border-box;outline:0;border:0;background:none;cursor:default;line-height:normal;color:inherit;font:inherit;text-align:inherit;overflow:visible;margin:0;border-radius:0;appearance:none;text-align:center;user-select:none;-webkit-touch-callout:none;background-color:#c6000e;color:#fff;padding:0 12px;line-height:48px;font-size:16px;height:48px;cursor:pointer;transform:translateZ(0);}
      .Donate .-submit-btn:disabled{animation:pulsate-opacity 1s ease-in-out infinite;}
      .Donate .-footer a{text-decoration:underline;}
      .Donate .-audit-badges{margin-left:-3px;}
      .Donate .-audit-badges svg{width:57px;height:57px;}
      .Donate .-audit-badges a{display:inline-block;margin-right:10px;text-decoration:none;}
      .Donate .-audit-links a{margin-right:5px;}
      .Donate .-audit-links {
        position: relative !important;
        top: 0 !important;
      }
      .-amount-input{box-sizing:border-box;outline:0;border:0;background:none;cursor:default;line-height:normal;color:inherit;font:inherit;text-align:inherit;overflow:visible;padding:0;margin:0;border-radius:0;appearance:none;}
      .-amount-btn,.-amount-input{font-size:16px;line-height:42px;height:42px;text-align:center;position:relative;transition:background-color .5s cubic-bezier(.19,1,.22,1),border-color .5s cubic-bezier(.19,1,.22,1);}
      .-amount-btn{border:1px solid hsla(0,0%,100%,.3);}
      .-amount-btn:first-of-type,.-amount-btn:nth-of-type(2){
        flex-basis:23%;
        max-width:23%;
        color: #fff;
      }
      .-amount-btn:nth-of-type(2){
        border-left-color:hsla(0,0%,100%,0);
        border-right-color:hsla(0,0%,100%,0);
      }
      .-amount-btn:nth-of-type(3){
        flex-basis:25%;
        max-width:25%;
        color: #fff;
      }
      .col {
        padding-left: 0;
      }
      .-amount-btn:before{content:"$";vertical-align:super;font-size:10px;line-height:1;margin-right:1px;}
      .-amount-btn:hover{background-color:hsla(0,0%,100%,.03);}
      .-amount-input-holder{position:relative;flex-basis:29%;max-width:29%;}
      .-amount-input-holder:before{position:absolute;content:"$";vertical-align:super;font-size:10px;z-index:20;top:12px;left:12px;line-height:1;}
      .-amount-input{color:#fff;padding:0 12px 0 18px;text-align:left;font-size:16px;line-height:1;border:1px solid hsla(0,0%,100%,0);cursor:text;background-color:hsla(0,0%,100%,.1);}
      .-amount-input::-webkit-input-placeholder{color:#fff;}
      .-amount-input::-moz-placeholder{color:#fff;}
      .-amount-input:-ms-input-placeholder{color:#fff;}
      .-amount-input:-moz-placeholder{color:#fff;}
      .-fieldset{background-color:rgba(0,0,0,.03);position:relative;display:flex;flex-wrap:wrap;border:1px solid #dcdcdc;padding:0;}
      .-label{flex:1;display:flex;line-height:66px;font-size:15px;color:#555;line-height:24px;font-size:14px;padding:8px 12px;}
      .-input-text,.-label,.-select{box-sizing:border-box;height:66px;}
      .-input-text,.-select{background:transparent;position:absolute;top:0;left:0;right:0;z-index:0;font-family:Menlo,Consolas,Monaco,monospace;font-size:15px;padding:20px 12px 0;margin:0;outline:0;appearance:none;-webkit-appearance:none;-moz-appearance:none;border:0;border-radius:0;width:100%;color:#333;text-indent:0;}
      .-select+label:after{
        font-family:dg-icons;
        content:"";
        font-size:16px;
        line-height:46px;
        position:absolute;
        text-align:center;
        top:20px;
        right:12px;
        z-index:0;
      }
      .-recurring label{padding:2rem 2rem 2rem calc(2rem + 34px);line-height:1.2;cursor:pointer;}
      .-checkbox{display:block;outline:0;-webkit-appearance:button;width:20px;height:20px;cursor:pointer;position:relative;box-sizing:border-box;position:absolute;top:1.875rem;left:2rem;margin:0;border:1px solid #fff;transition:opacity .7s cubic-bezier(.19,1,.22,1);opacity:.3;}
      .-checkbox:checked,.-checkbox:checked+label:after{opacity:1;}
      .-checkbox+label:after{
        box-sizing:border-box;
        font-family:dg-icons;
        content:"";
        text-align:center;
        line-height:20px;
        font-size:16px;
        position:absolute;
        top:2.175rem;
        left:2rem;
        width:20px;
        height:20px;
        transition:opacity .7s cubic-bezier(.19,1,.22,1);
        color:#fff;
        opacity:0;
      }
      .-payment .-input-text{font-family:Menlo,Consolas,Monaco,monospace;}
      .-input-text+label,.-input-text:focus+label,.-input-text:valid+label{color:#555;}
      .-address,.-address-2,.-city,.-country,.-credit-card,.-cvc,.-email,.-expiration,.-name,.-state,.-zip{position:relative;}
      .-address,.-address-2,.-credit-card,.-email,.-name{flex-basis:100%;max-width:100%;}
      .-city,.-country,.-cvc,.-expiration,.-state,.-zip{flex-basis:50%;max-width:50%;}
      .-address,.-address-2,.-city,.-credit-card,.-name,.-state{border-bottom:1px solid #dcdcdc;}
      .-country,.-cvc,.-state{border-left:1px solid #dcdcdc;}
      .-website{display:none!important;}
      @media screen and (max-width:640px){
      .Donate .-header{border:0;padding-right:1rem;}
      .Donate .-main{border:0;padding-left:1rem;}
      .Donate .-dg-brand{order:3;}
      .Donate .-other-ways{padding-left:1rem;}
      .Donate .-dg-brand,.Donate .-stewardship{padding-top:2rem;padding-bottom:2rem;border-top:1px solid #dcdcdc;}
      }
