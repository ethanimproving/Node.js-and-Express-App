style.
  /* loader css */
  .loader-wrapper {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    display:flex;
    justify-content: center;
    align-items: center;
  }
  .loader {
    display: inline-block;
    width: 30px;
    height: 30px;
    position: relative;
    border: 4px solid #Fff;
    animation: loader 2s infinite ease;
  }
  .loader-inner {
    vertical-align: top;
    display: inline-block;
    width: 100%;
    background-color: #fff;
    animation: loader-inner 2s infinite ease-in;
  }
  @keyframes loader {
    0% { transform: rotate(0deg);}
    25% { transform: rotate(180deg);}
    50% { transform: rotate(180deg);}
    75% { transform: rotate(360deg);}
    100% { transform: rotate(360deg);}
  }
  @keyframes loader-inner {
    0% { height: 0%;}
    25% { height: 0%;}
    50% { height: 100%;}
    75% { height: 100%;}
    100% { height: 0%;}
  }

  /*! CSS Used from: Embedded */
  .preloader{
    position:fixed;
    top:0;
    left:0;
    width:100vw;
    height:100vh;
    z-index:2000;
    overflow:hidden;
  }
  .preloader .preloader-bg{
    position:absolute;
    z-index:0;
    top:0;
    left:0;
    width:100vw;
    height:100vh;
    transform-origin:50% 100%;
    background:#242f3f;
    overflow:hidden;
  }

  .preloader .preloader-bg>div{
    position:absolute;
    z-index:0;
    top:0;
    left:0;
    width:100vw;
    height:100vh;
    background-repeat:repeat;
    background-size:320px 343px;
    opacity:0;
    transform:scale(1.4);
    transition:opacity .5s linear,transform 2s cubic-bezier(0,.785,.225,.995);
  }
  .preloader .preloader-bg>div.visible{
    opacity:.175;
    transform:scale(1);
  }
  .preloader .preloader-content{
    position:absolute;
    z-index:1;
    top:50%;
    left:50%;
    transform-origin:50% 50%;
    transform:translate(-50%,-50%);
    height: 130px;
    width: 130px;
  }
  .preloader figure{
    background:#fff;
    padding:20px 20px;
    border-radius:100px;
  }
  .preloader img{
    display:block;
    width:100%;
    height:100%;
    transform: scale(1.5);
    border-radius: 50%;
  }
  .preloader p{
    z-index:3;
    position:fixed;
    bottom:30px;
    opacity:0;
    width:100vw;
    font-family:Rajdhani,sans-serif;
    color:#fff;
    font-weight:800;
    font-size:18px;
    letter-spacing:.1em;text-align:center;margin-top:20px;text-transform:uppercase;
  }
  /*! CSS Used from: http://anafi.parrot.com/assets/builds/bundle.css */
  *{
    margin:0;
    padding:0;
    border-collapse:inherit;
  }
  *,:after,:before{
    box-sizing:inherit;
  }
  :not(body){
    background-repeat:no-repeat;
    background-position:50%;
    background-size:cover;
  }
  *{
    box-sizing:border-box;
  }

  /* Event Changes */
  .preloader-msg {
    opacity: 1;
    transform: translateY(0px);
  }

  .preloader-bg.height-animate {
    background: #242f3f;
    transform: scaleY(0);
    -webkit-transition: all 700ms cubic-bezier(0.605, 0.020, 0.000, 1.035); 
  }

  .preloader .shrink {
    transform: scale(0);
    opacity: 0 !important;
    -webkit-transition: all 300ms ease-out;
  }

